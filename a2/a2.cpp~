#include <iostream>
#include <fstream>
#include <string>

using namespace std;

void readInput();

int main(){
	readInput();
}

void readInput(){
	string firstFile, secondFile;
	cout << "Enter the name of the first file: ";
	getline(cin, firstFile);
	cout << firstFile << endl;
	cout << "Enter the name of the second file: ";
	getline(cin, secondFile);
	cout << secondFile << endl;
}
